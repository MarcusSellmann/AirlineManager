<Window x:Class="AirlineManager.UI.SubViews.AirportExplorerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AirlineManager.UI.SubViews"
        mc:Ignorable="d"
        Title="AirportExplorerWindow" Height="400" Width="600">
    <DockPanel
        xmlns:amb="clr-namespace:AirlineManager.Business;assembly=AirlineManagerBusinessLogic"
        xmlns:amd="clr-namespace:AirlineManager.Data;assembly=AirlineManagerData">
        <DockPanel.Resources>
            <amd:Airport x:Key="AP"/>
        </DockPanel.Resources>
        <DockPanel.DataContext>
            <Binding Source="{StaticResource }"
        </DockPanel.DataContext>
        <Menu DockPanel.Dock="Top">
            <MenuItem Header="_File">
                <MenuItem Header="_New" />
            </MenuItem>
            <MenuItem Header="_Airports">
                <MenuItem Header="_Explore" />
            </MenuItem>
        </Menu>
        <ListBox ItemsSource="{Binding}">
            
        </ListBox>
    </DockPanel>
</Window>
